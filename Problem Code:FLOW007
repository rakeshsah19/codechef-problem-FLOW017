# codechef-problem-FLOW007
/* problem on codechef Problem Code:FLOW007*/
#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n,rev=0,rem;
	    cin>>n;
	    while(n!=0){
	        rem=n%10;
	        rev=rev*10+rem;
	        n=n/10;
	    }
	    cout<<rev<<endl;
	}
	
	return 0;
}
